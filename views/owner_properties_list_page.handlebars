<main>
    <div class="ownerLinks">  
        <a href="/owners" class="studentElement2">Profile</a>
        <a href="/sign-out" class="studentElement2">Sign-out</a>
        <br>
        <br>
    </div> 
   
   <div class="properties">    
        {{#each data}}
        <a href="/properties/viewProperty/{{_id}}" class="propertyLink">
            {{#each images}}
            <img src="{{url}}" alt="Property Image" width="100" height="100">
            {{/each}}
            <p>Address: {{address}}</p>
            <p>Rent: {{rent}} </p>
            <p>Bed: {{bed}} </p>
            <p>Bath: {{bath}} </p>
        </a> 
        <br>
        {{/each}}
    </div>
    
    <form action="/properties/createProperty" method="get">
        <label for="Add-Property"></label>
        <input type="submit" id="Add-Property" value="Add Property" class="btn btn-primary">
    </form>
</main>